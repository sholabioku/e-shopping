{
  "extends": ["airbnb-base"],
  "rules": {
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "consistent-return": 0,
    "func-names": 0,
    "no-restricted-syntax": 0,
    "node/no-unpublished-require": 0,
    "linebreak-style": 0,
    "no-undef": 0,
    "no-shadow": 1,
    "max-len": 1,
    "global-require": 1,
    "prefer-const": 1,
    "prefer-destructuring": 1,
    "no-useless-concat": 1,
    "no-param-reassign": 1,
    "prefer-promise-reject-errors": 1,
    "implicit-arrow-linebreak": 0,
    "no-use-before-define": 0
  }
}
